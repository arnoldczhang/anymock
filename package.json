{
  "name": "anymock",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "start": "vite --host",
    "dev": "vite",
    "clear": "rm -f dist/js/*.ts",
    "build:script": "esbuild ./public/js/script.ts ./public/js/content.ts --bundle --minify --sourcemap --format=iife --outdir=dist/js/ && pnpm clear",
    "compile": "node ./compile-theme.cjs",
    "build": "vue-tsc --noEmit && vite build && pnpm build:script",
    "preview": "vite preview"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/view": "^6.9.5",
    "@element-plus/icons-vue": "^2.0.10",
    "@vueuse/core": "^9.4.0",
    "codemirror": "^6.0.1",
    "element-plus": "^2.2.32",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.33",
    "uuid": "^8.3.2",
    "vue": "^3.2.37",
    "vue-codemirror": "^6.1.1",
    "vue-router": "^4.1.3",
    "xhook": "^1.5.5"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.193",
    "@types/lodash": "^4.14.182",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^14.16.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^7.0.0",
    "dart-sass": "^1.25.0",
    "esbuild": "^0.14.53",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-vue": "^7.19.1",
    "less": "^4.1.3",
    "prettier": "2.4.1",
    "prettier-eslint": "^13.0.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.8"
